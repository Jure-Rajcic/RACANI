{
  "name": "@auth-demo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "_build:admin": "nx build admin-app --configuration=production --skip-nx-cache",
    "_build:client": "nx build client-app --configuration=production --skip-nx-cache",
    "_build:all": "npm run clean && npm run _build:admin && npm run _build:client",
    "_deploy:admin": "firebase deploy --only hosting:admin-app",
    "_deploy:client": "firebase deploy --only hosting:client-app",
    "_deploy:all": "npm run _deploy:admin && npm run _deploy:client",
    "build-deploy:all": "npm run _build:all && npm run _deploy:all",
    "start:admin": "nx run admin-app:serve",
    "start:client": "nx run client-app:serve",
    "setup-firebase": "node setup-firebase.js",
    "emulators:build": "cd firebase/functions && npm run build",
    "emulators:start": "npm run emulators:reset && npm run emulators:build && firebase emulators:start --only functions,firestore,database",
    "emulators:debug": "npm run emulators:reset && npm run emulators:build && firebase emulators:start --only functions,firestore,database --inspect-functions",
    "emulators:seed": "ts-node -P firebase/seed/tsconfig.json -r tsconfig-paths/register firebase/seed/src/index.ts",
    "emulators:reset": "lsof -ti:4000,8000,9000,5001,8080 | xargs kill -9 2>/dev/null || true"
  },
  "private": true,
