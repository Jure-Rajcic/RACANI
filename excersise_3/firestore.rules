rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are completely open and should only be used for local development.
    // Anyone can read, write, and delete any data in your database.
    // Make sure to secure your rules before deploying to production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
